<?xml version="1.0" encoding="UTF-8"?>

<mule
	xmlns:secure-properties="http://www.mulesoft.org/schema/mule/secure-properties"
	xmlns:snowflake="http://www.mulesoft.org/schema/mule/snowflake"
	xmlns:db="http://www.mulesoft.org/schema/mule/db"
	xmlns:http="http://www.mulesoft.org/schema/mule/http"
	xmlns="http://www.mulesoft.org/schema/mule/core"
	xmlns:doc="http://www.mulesoft.org/schema/mule/documentation"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
http://www.mulesoft.org/schema/mule/db http://www.mulesoft.org/schema/mule/db/current/mule-db.xsd 
http://www.mulesoft.org/schema/mule/http http://www.mulesoft.org/schema/mule/http/current/mule-http.xsd http://www.mulesoft.org/schema/mule/core http://www.mulesoft.org/schema/mule/core/current/mule.xsd
http://www.mulesoft.org/schema/mule/snowflake http://www.mulesoft.org/schema/mule/snowflake/current/mule-snowflake.xsd
http://www.mulesoft.org/schema/mule/secure-properties http://www.mulesoft.org/schema/mule/secure-properties/current/mule-secure-properties.xsd">
	<snowflake:snowflake-config
		name="larson_snowflake" doc:name="Snowflake Config"
		doc:id="85238398-d54f-48c4-a984-2cebc5ddd262">
		<snowflake:snowflake-connection
			accountName="${larson.account_name}" warehouse="${larson.warehouse}"
			database="${larson.database}" schema="${larson.schema2}"
			user="${larson.snowflake_username}"
			password="${secure::larson.snowflake_password}" role="${larson.role}" />
	</snowflake:snowflake-config>
	<db:config name="larson_source"
		doc:name="Database Config"
		doc:id="313798d6-8976-450a-8a8a-db47b654c8f1">
		<db:mssql-connection host="${larson.host}" user="${larson.user}" password="${secure::larson.password}"/>

	</db:config>
	<configuration-properties
		doc:name="Configuration properties"
		doc:id="08099691-3e90-4ddc-a377-8bc1c24398a2"
		file="properties\test.config.yaml" />
	<secure-properties:config
		name="Secure_Properties_Config" doc:name="Secure Properties Config"
		doc:id="3fa08474-19c1-4926-8288-ccaf4364a95d"
		file="properties\test.config.yaml" key="${secretkey}" />
	<global-property doc:name="Global Property"
		doc:id="707868d7-5635-4666-951f-92f58a50919a" name="secretkey"
		value="owlcbelarsonwildco123456" />

</mule>